#include <stdio.h>
int main()
{
    int num;
    printf("Enter an number from 1 to 5: ");
    scanf("%d", &num);
    switch(num)
    {
        case 1:
            printf("Food item: Pizza\n");
            printf("Food price: Rs 239\n");
            break;
        case 2:
            printf("Food item: Burger\n");
            printf("Food price: Rs 129\n");
            break;
        case 3:
            printf("Food item: Pasta\n");
            printf("Food price: Rs 179\n");
            break;
        case 4:
            printf("Food item: French Fries\n");
            printf("Food price: Rs 99\n");
            break;
        case 5:
            printf("Food item: Sandwich\n");
            printf("Food price: Rs 149\n");
            break;
    }
}
